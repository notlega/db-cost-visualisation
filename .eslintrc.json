{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "next/core-web-vitals"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/no-unresolved": "off",
    "react/jsx-props-no-spreading": "off",
    "object-curly-newline": "off"
  }
}
